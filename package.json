    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-markdown": "^3.1.0",
    "react-redux": "^5.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.3.0",
    "xlsx": "^0.11.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
